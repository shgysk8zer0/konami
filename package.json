{
  "name": "@shgysk8zer0/konami",
  "version": "1.0.10",
  "description": "A simple JavaScript module for adding Konami Code Easter Eggs",
  "main": "konami.cjs",
  "module": "konami.js",
  "private": false,
  "type": "module",
  "engines": {
    "node": ">=15.3.0"
  },
  "exports": {
    ".": {
      "import": "./konami.js",
      "require": "./konami.cjs"
    }
  },
  "scripts": {
    "test": "eslint .",
    "preversion": "npm test",
    "prepare": "npm run build",
    "build": "npm run build:js",
    "build:js": "rollup -c rollup.config.js",
    "version:bump": "npm run version:bump:patch",
    "version:bump:patch": "npm version --no-git-tag-version patch",
    "version:bump:minor": "npm version --no-git-tag-version minor",
    "version:bump:major": "npm version --no-git-tag-version major",
    "fix:js": "eslint --fix .",
    "fix": "npm run fix:js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shgysk8zer0/konami.git"
  },
  "keywords": [
    "konami",
    "konami-js",
    "konami-code",
    "easter-egg"
  ],
  "author": "Chris Zuber <admin@kernvalley.us>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shgysk8zer0/konami/issues"
  },
  "homepage": "https://shgysk8zer0.github.io/konami/",
  "devDependencies": {
    "@shgysk8zer0/js-utils": "^1.0.0"
  }
}
